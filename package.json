    "gatsby-plugin-typescript": "^2.5.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-source-vimeo": "github:lordoffreaks/gatsby-source-vimeo",
    "gatsby-transformer-sharp": "^2.5.21",
    "react": "^16.14.0",
    "react-burger-menu": "^2.9.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-player": "^1.15.3",
    "react-redux": "^7.2.2",
    "react-scroll": "^1.8.1",
    "react-show-more-text": "^1.4.6",
