    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-source-vimeo": "github:lordoffreaks/gatsby-source-vimeo",
    "gatsby-transformer-json": "^2.2.16",
    "gatsby-transformer-sharp": "^2.3.2",
    "react": "^16.11.0",
    "react-burger-menu": "^2.6.11",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-player": "^1.12.0",
    "react-redux": "^7.1.1",
    "react-scroll": "^1.7.14",
