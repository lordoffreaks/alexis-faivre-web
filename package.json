{
  "name": "alexis-faivre-web",
  "description": "Personal website for Alexis Faivre.",
  "version": "1.0.0",
  "private": true,
  "author": "Alejandro Tabares <info@amajua.com>",
  "keywords": [
    "gatsby",
    "editing",
    "direction",
    "video"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "check": "tsc --noEmit",
    "clean": "rimraf public .cache",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "predev": "npm run clean",
    "dev": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "pretest": "npm run type-check",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "preserve": "npm run build",
    "serve": "gatsby serve --host=0.0.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.18",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@researchgate/react-intersection-observer": "^1.3.5",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-canonical-urls": "^2.3.13",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-react-helmet": "^3.3.14",
