{
  "name": "alexis-faivre-web",
  "description": "Personal website for Alexis Faivre.",
  "version": "1.0.0",
  "private": true,
  "author": "Alejandro Tabares <info@amajua.com>",
  "keywords": [
    "gatsby",
    "editing",
    "direction",
    "video"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "clean": "rimraf public .cache",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "predev": "npm run clean",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc --noEmit",
    "preserve": "npm run build",
    "serve": "gatsby serve"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@researchgate/react-intersection-observer": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "dotenv": "^8.2.0",
    "gatsby": "^2.17.7",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-canonical-urls": "^2.1.13",
    "gatsby-plugin-manifest": "^2.2.26",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-react-helmet": "^3.1.13",
