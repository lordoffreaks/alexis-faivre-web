{
  "name": "alexis-faivre-web",
  "description": "Personal website for Alexis Faivre.",
  "version": "1.0.0",
  "private": true,
  "author": "Alejandro Tabares <info@amajua.com>",
  "keywords": [
    "gatsby",
    "editing",
    "direction",
    "video"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "clean": "rimraf public .cache",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "predev": "npm run clean",
    "dev": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "pretest": "npm run type-check",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "preserve": "npm run build",
    "serve": "gatsby serve --host=0.0.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.16",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@researchgate/react-intersection-observer": "^1.0.5",
    "clsx": "^1.0.4",
    "dotenv": "^8.2.0",
    "gatsby": "^2.18.14",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-canonical-urls": "^2.1.18",
    "gatsby-plugin-manifest": "^2.2.33",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sharp": "^2.3.9",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-source-vimeo": "github:lordoffreaks/gatsby-source-vimeo",
    "gatsby-transformer-sharp": "^2.3.9",
    "react": "^16.12.0",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-player": "^1.14.2",
    "react-redux": "^7.1.3",
    "react-scroll": "^1.7.14",
    "react-spring": "^8.0.27",
